#include <iostream>>

using namespace std;

int main()
{
    cout << "\n\: \n\n";

    cout << "\n\t\xDC\xDC\xDB\xDB\xDB\xDB\xDC\xDC";
    cout << "\n\t\xDFO\xDF\xDF\xDF\xDFO\xDF" << endl << endl;

    cout << "\n\t \n\n";

    cout << "\n\t\xDC\xDC\xDB \xDB\xDB\xDB\xDB\xDB\xDB";
    cout << "\n\t\xDFO\xDF\xDF\xDF\xDF\xDFOO\xDF" << endl << endl;

    return 0;
}
